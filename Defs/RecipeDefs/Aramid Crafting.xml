<?xml version="1.0" encoding="UTF-8"?>

<Defs>
    <!-- devil strand fibre x25 to Aramid x25-->
    <RecipeDef>
        <defName>Make_Aramid_devilstrand</defName>
        <label>make aramid from devilstrand</label>
        <description>Weave Devilstrand fibres into Aramid</description>
        <jobString>Weaving Aramid.</jobString>
        <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
        <effectWorking>Tailor</effectWorking>
        <soundWorking>Recipe_Tailor</soundWorking>
        <workAmount>750</workAmount>
        <allowMixingIngredients>False</allowMixingIngredients>
        <targetCountAdjustment>25</targetCountAdjustment>
        <recipeUsers>
          <li>ElectricTailoringBench</li>
          <li>HandTailoringBench</li>
        </recipeUsers>
        <ingredients>
            <li>
              <filter>
                <thingDefs>
                  <li>Snek_DevilstrandFibre</li>
                </thingDefs>
              </filter>
              <count>25</count>
            </li>
        </ingredients>
        <fixedIngredientFilter>
            <thingDefs>
              <li>DevilstrandCloth</li>
            </thingDefs>
        </fixedIngredientFilter>
        <products>
          <DevilstrandCloth>25</DevilstrandCloth>
        </products>
        <skillRequirements>
            <Crafting>4</Crafting>
          </skillRequirements>
        <workSkill>Crafting</workSkill>
        <displayPriority>0</displayPriority>
    </RecipeDef>
